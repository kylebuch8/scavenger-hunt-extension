{
  "manifest_version": 2,
  "name": "Customer Portal Scavenger Hunt",
  "description": "A scavenger hunt on the Customer Portal built for the 2017 Red Hat Summit",
  "version": "1.0",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://access.redhat.com/*"],
      "js": ["main.js"]
    }
  ],
  "web_accessible_resources":[
    "bower_components/**/*",
    "scavengerhunt-app.html",
    "main.js"
  ]
}
